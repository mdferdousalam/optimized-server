{
    "version": 2,
   "rewrites": [
    {
        "source": "/(.*)",
        "destination": "/src"
    }
],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/index.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}